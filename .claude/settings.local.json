      "Bash(next.config \")",
      "Bash(npm install:*)",
      "Bash(timeout /t 5 /nobreak)",
      "Bash(powershell.exe -Command \"Stop-Process -Id 15952 -Force\")",
      "Bash(if exist .nextdev rmdir /s /q .nextdev)"
